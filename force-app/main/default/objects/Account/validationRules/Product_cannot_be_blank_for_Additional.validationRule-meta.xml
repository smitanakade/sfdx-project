<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Product_cannot_be_blank_for_Additional</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISNEW(), ISPICKVAL(Account_Type__c, &#39;Additional ECN&#39;), ISBLANK(eNett_products__c))</errorConditionFormula>
    <errorDisplayField>eNett_products__c</errorDisplayField>
    <errorMessage>You must select a product before saving</errorMessage>
</ValidationRule>
