<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_set_Priority_to_None_for_email</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(Status), ISPICKVAL(Priority, &quot;-- None --&quot;))</errorConditionFormula>
    <errorMessage>--None-- is not an acceptable Customer Service Priority value.  Please select a valid option from the list.</errorMessage>
</ValidationRule>
