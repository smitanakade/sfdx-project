<?xml version="1.0" encoding="UTF-8"?>
<BusinessProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Reopen Ticket Support Process</fullName>
    <description>For reopened tickets, with only Pending, reopen statuses (and closed) available</description>
    <isActive>true</isActive>
    <values>
        <fullName>Closed</fullName>
        <default>false</default>
    </values>
    <values>
        <fullName>Pending</fullName>
        <default>false</default>
    </values>
    <values>
        <fullName>Pending with AML</fullName>
        <default>false</default>
    </values>
    <values>
        <fullName>Reopen</fullName>
        <default>true</default>
    </values>
</BusinessProcess>
